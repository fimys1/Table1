        xmlns:local="clr-namespace:WpfApplication"
        mc:Ignorable="d"
        Title="WindowChangeTable" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="10,28,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
